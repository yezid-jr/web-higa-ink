{
  
  "builds": [
    { "src": "vite.config.ts", "use": "@vercel/vite" }
  ],
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/src/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}